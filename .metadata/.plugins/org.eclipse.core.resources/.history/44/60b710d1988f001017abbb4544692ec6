<button type="button" onclick="changeColor()">스크립트</button>

<script>
function changeColor() {
	let userName = prompt("수정할 글");
	let pwd = prompt("pwd");


	if (userName !== null) {
		location.replace("../board/view.jsp?idx="+userName+"&pwd="+pwd);
	} else {
	  	console.log("이름 입력이 취소되었습니다.");
	}
}

</script>
<script>
function toggleSubMenu(submenuId) {
    var submenu = document.getElementById(submenuId);
    if (submenu.style.display === 'block') {
      submenu.style.display = 'none';
    } else {
      submenu.style.display = 'block';
    }
  }
  </script>