<script>
let userName = prompt("당신의 이름은 무엇인가요?", "1");
let pwd = prompt("비밀번호를 입력하세요", "2");


if (userName !== null) {
	location.replace("../board/view.jsp?idx="+userName+"&pwd="+ "<%= pwd %>");
} else {
  	console.log("이름 입력이 취소되었습니다.");
}
</script>